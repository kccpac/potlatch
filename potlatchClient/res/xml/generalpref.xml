<?xml version="1.0" encoding="UTF-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- NOTE: ListPreference's summary should be set to its value by the activity code. -->
    <ListPreference
        android:defaultValue="-1"
        android:entries="@array/pref_refresh_rate_list_titles"
        android:entryValues="@array/pref_refresh_rate_list_values"
        android:key="refresh_rate_list"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="@string/pref_title_refresh_rate" />
    <ListPreference
        android:defaultValue="-1"
        android:entries="@array/prohabit_gift_list_titles"
        android:entryValues="@array/prohabit_gift_list_values"
        android:key="prohabit_type_list"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="@string/pref_title_prohabit_gift" />

</PreferenceScreen>